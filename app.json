{
  "expo": {
    "name": "BuddyAlert",
    "slug": "buddyalert",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "buddyalert",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/bfce077b-c37e-40aa-adce-bbc7464b6ade"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.christophebraun.buddyalert",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "We use this to improve notifications relevance.",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ]
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.christophebraun.buddyalert"
    },
    "plugins": [
      "expo-font",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "bfce077b-c37e-40aa-adce-bbc7464b6ade"
      },
      "oneSignalAppId": "28190a53-0cd4-403d-b9dd-99c8c1c7717c"
    },
    "owner": "cbraun",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
