{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "COCOAPODS_VERBOSE": "true",
        "EXPO_DEBUG": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "hooks": {
      "pre-install": "bash ./scripts/eas-pre-install.sh",
      "post-install": "bash ./scripts/eas-post-install.sh"
    }
  },
  "submit": {
    "production": {}
  }
}
